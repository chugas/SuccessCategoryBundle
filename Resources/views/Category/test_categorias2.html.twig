{% extends 'SonataAdminBundle:CRUD:list.html.twig' %}

{% block stylesheets %}
  {{ parent() }}
  <link href="/bundles/test/css/categorias/associate/dynatree/ui.dynatree.css" rel="stylesheet" type="text/css" id="skinSheet">  
{% endblock %}

{% block javascripts %}
  {{ parent() }}
  <script src="/bundles/test/js/categorias/associate/dynatree/jquery.dynatree.js" type="text/javascript"></script>
{% endblock %}

{% block list_table %}

  {{ results | raw }}

  <script type="text/javascript">
    $(function() {      
      $("#tree").treeview({
        collapsed: true,
        animated: "medium",
        control:"#sidetreecontrol",
        persist: "location"
      });      
    });
  </script>
{% endblock %}
